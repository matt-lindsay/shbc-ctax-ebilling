<div class="col-md-6 col-md-offset-3">
    <form action="/" method="post" id="addressSearchForm">
        <div class="form-group">
            <div>
                <label>Address</label>
            </div>
            <div id="custom-templates" class="form-group">
                <input class="typeahead form-control" type="text" name="data" placeholder="Address search" />
                <input class="typeahead form-control" type="hidden" name="uprn" placeholder="Uprn" />
            </div>
        </div>
        <div class="form-group">
            <label for="account#">Council tax account number</label>
            <input type="text" id="accountNo" class="form-control" name="accountNo" placeholder="Account #" />
        </div>
        <div class="form-group">
            <label>Email</label>
            <input type="email" id="email" class="form-control" name="email" placeholder="Email" />
        </div>
        <div class="form-group">
            <a href="/" class="btn btn-warning">Cancel</a>
            <input type="submit" id="submit" class="btn btn-primary" value="Submit" />
        </div>
    </form>
</div>